            <fileset dir="${dir.jarfile}"/>
        </jar>
        <delete dir="${dir.jarfile}/Kexdealers_lib"/>
        <mkdir dir="${dir.jarfile}/Kexdealers_lib"/>
        <copy file="${dir.buildfile}/lib/jars/lwjgl.jar" todir="${dir.jarfile}/Kexdealers_lib"/>
        <copy file="${dir.buildfile}/lib/jars/lwjgl-glfw.jar" todir="${dir.jarfile}/Kexdealers_lib"/>
        <copy file="${dir.buildfile}/lib/jars/joml-1.9.4.jar" todir="${dir.jarfile}/Kexdealers_lib"/>
        <copy file="${dir.buildfile}/lib/jars/lwjgl-openal.jar" todir="${dir.jarfile}/Kexdealers_lib"/>
